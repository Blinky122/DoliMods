{
  "manifest_version": 2,
  "name": "Dolibarr ERP & CRM in the Cloud",
  "description": "Dolibarr ERP & CRM, OpenSource software for business (invoices, customers, suppliers, proposals, agenda, products, emailings...)",
  "default_locale": "en",
  "version": "3.2.2.6",
  "app": {
    "urls": [
      "http://www.dolicloud.com/",
      "https://www.dolicloud.com/"
    ],
    "launch": {
      "web_url": "http://www.dolicloud.com/chromewebstore/index.php"
    }
  },
  "icons": {
    "16": "appicon_16.png", "64": "appicon_64.png", "128": "appicon_128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "notifications"
  ]
}
